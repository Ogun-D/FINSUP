<div class="container">
  <div class="row justify-content-center">
    <div class="col-12 col-lg-6">
      <div class="dashboard-background">
        <h1>Dashboard</h1>

        <% @requests.each do |r| %>
          <p><%= r.advisor.first_name %> <%= r.advisor.last_name %> </p>
          <p>This should be a card with request details. If you click it you go to the chat with advisor</p>
          <%= link_to 'Chat', chat_request_path(r) %>
        <% end %>
      </div>
    </div>
  </div>
</div>
